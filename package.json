{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "build": "cross-env nest build",
    "format": "cross-env prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env nest start",
    "start:dev": "cross-env nest start --watch",
    "start:debug": "cross-env nest start --debug --watch",
    "start:prod": "cross-env node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env jest",
    "test:watch": "cross-env jest --watch",
    "test:cov": "cross-env jest --coverage",
    "test:debug": "cross-env node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-esm -r tsconfig-paths/register node_modules\\typeorm\\cli.js --dataSource src/database/data-source.ts",
    "premigration:generate": "npm run build",
    "premigration:run": "npm run build",
    "migration:create": "typeorm migration:create",
    "migration:generate": "typeorm migration:generate -d dist/database/data-source -p",
    "migration:run": "typeorm migration:run -d dist/database/data-source",
    "migration:revert": "typeorm migration:revert -d dist/database/data-source"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.3.0",
    "joi": "^17.13.3",
    "magic-bytes.js": "^1.11.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.15.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.1",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@babel/helpers": "^7.27.0",
    "@biomejs/biome": "^1.9.4",
    "@faker-js/faker": "^9.5.1",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/swagger": "^11.1.5",
    "@swc/cli": "^0.7.3",
    "@swc/core": "^1.11.22",
    "@types/bytes": "^3.1.5",
    "@types/express": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "helmet": "^8.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
